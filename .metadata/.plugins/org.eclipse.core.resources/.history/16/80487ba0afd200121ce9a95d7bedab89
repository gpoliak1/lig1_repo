#include <stdio.h>

#include <stdlib.h>
#include "SimpleNurbs.h"
#include <math.h>
#include <limits.h>
#include <GL/glut.h>

#include <cstdlib> // for rand() and srand()
#include <ctime> // for time()

float genRand() {

	float f = (float) (rand() % 32767) / (float) 32767;

	return f;
}


